<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bintang.tripapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TripApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".introScreen.onBoardingThree"></activity>
        <activity android:name=".introScreen.onBoardingOne" />
        <activity android:name=".introScreen.onBoardingTwo" />
        <activity android:name=".introScreen.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.ProfileActivity" />
        <activity android:name=".main.DetailActivity" />
        <activity android:name=".main.MainActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>